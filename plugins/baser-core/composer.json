{
    "name": "baserproject/baser-core",
    "description": "baserCMS plugin for CakePHP",
    "homepage": "https://basercms.net",
    "type": "cakephp-plugin",
    "license": "MIT",
    "vendor-dir": "../../vendor",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "4.4.*",
        "cakephp/authentication": "^2.9",
        "cakephp/migrations": "^3.2",
        "doctrine/annotations": "^1.12",
        "firebase/php-jwt": "6.1.0",
        "josegonzalez/dotenv": "^3.2",
        "mobiledetect/mobiledetectlib": "^2.8",
        "ext-json": "*",
        "baserproject/bc-admin-third": "5.0.*",
        "baserproject/bc-blog": "5.0.*",
        "baserproject/bc-content-link": "5.0.*",
        "baserproject/bc-custom-content": "5.0.*",
        "baserproject/bc-editor-template": "5.0.*",
        "baserproject/bc-favorite": "5.0.*",
        "baserproject/bc-front": "5.0.*",
        "baserproject/bc-installer": "5.0.*",
        "baserproject/bc-mail": "5.0.*",
        "baserproject/bc-search-index": "5.0.*",
        "baserproject/bc-theme-config": "5.0.*",
        "baserproject/bc-theme-file": "5.0.*",
        "baserproject/bc-uploader": "5.0.*",
        "baserproject/bc-widget-area": "5.0.*",
        "imo-tikuwa/cakephp-soft-delete": "^2.0",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "cakephp/bake": "^2.6",
        "cakephp/debug_kit": "^4.5",
        "vierge-noire/cakephp-fixture-factories": "^2.5",
        "vierge-noire/cakephp-test-suite-light": "^2.0",
        "ext-xdebug": "*"
    },
    "autoload": {
        "psr-4": {
            "BaserCore\\": "src",
            "Cake\\Routing\\": "src/Routing"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BaserCore\\Test\\": "tests/"
        }
    }
}
